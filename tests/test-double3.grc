void testdouble(double a, double b)
{
  printf(a + b);
  printf(a - b);
  printf(a * b);
  printf(a / b);
  printb(a == b);
  printb(a == a);
  printb(a != b);
  printb(a != a);
  printb(a > b);
  printb(a >= b);
  printb(a < b);
  printb(a <= b);
}

int main()
{
  double c;
  double d;

  c = 42.0;
  d = 3.14159;

  testdouble(c, d);

  testdouble(d, d);

  return 0;
}
