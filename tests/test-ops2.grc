void printb(bool b){
  if (b){
    printi(1);
  }
  else {
    printi(0);
  }
}
int main()
{
  printb(True);
  printb(False);
  printb(True && True);
  printb(True && False);
  printb(False && True);
  printb(False && False);
  printb(True || True);
  printb(True || False);
  printb(False || True);
  printb(False || False);
  printb(!False);
  printb(!True);
  printi(-10);
  printi(--42);
}
